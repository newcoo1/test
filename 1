git diff    //see what u changed but not yet staged
git diff --staged   //see changes staged ,also --cached
git commit -a	//auto stage news and commit
git rm 
git rm --cached filename    //untrack file but keep on hard drive
git mv	    //rename a file
git log	    //show history
git log -p -2	    //show history with diff and limits the output to only the last 2 entries

